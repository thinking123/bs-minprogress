
<view class="container">
    <view class="user-avatar">
        <open-data type="userAvatarUrl"></open-data>
    </view>
    <view class="user-nick">
        <open-data type="userNickName"></open-data>
    </view>

    <image src="{{url}}bg.png" class="img"/>
    <view class="singer-home" bindtap="handleSingerHome"/>
    <view class="my-achieve" bindtap="handleMyAchieve"/>
    <view class="win-info" bindtap="handleWinInfo"/>
    <view class="attention-list" bindtap="handleAttentionList"/>
    <image class="review-info"
           src="{{url}}review-btn.png"
           bindtap="handleReviewInfo">
        审核信息
    </image>
    <upload-type-select-dialog
            bind:hidetap="handleHideDialog"
            bind:submit="handleSubmitDialog"
            visible="{{showUploadDialog}}"/>

    <!--<check-error-ex-dialog-->
            <!--bind:rewrite="handleReWrite"-->
            <!--content="{{errorMsg}}"-->
            <!--bind:hidetap="handleCheckErrorExDialog"-->
            <!--visible="{{showCheckErrorExDialog}}"/>-->
    <singer-home-no-music-dialog
            bind:hidetap="handleHideNoMusicDialog"
            visible="{{showNoMusicDialog}}"/>
    <check-error-dialog
            bind:rewrite="handleReWrite"
            content="{{errorMsg}}"
            bind:hidetap="handleCheckErrorDialog"
            visible="{{showCheckErrorDialog}}"/>
    <checking-dialog
            bind:hidetap="handleCheckIngDialog"
            visible="{{showCheckIngDialog}}"/>
    <check-success-dialog
            bind:hidetap="handleCheckSuccessDialog"
            visible="{{showCheckSuccessDialog}}"/>
    <check-success-but-music-dialog
            bind:hidetap="handleCheckSuccessButMusicDialog"
            visible="{{showCheckSuccessButMusicDialog}}"/>
    <signin-check-dialog
            bind:hidetap="handleSigninCheckDialog"
            visible="{{showSigninCheckDialog}}"/>
    <singer-home-checking-dialog
            bind:hidetap="handleSingerHomeCheckingDialog"
            visible="{{showSingerHomeCheckingDialog}}"/>
    <singer-home-checking-error-dialog
            bind:hidetap="handleSingerHomeCheckingErrorDialog"
            visible="{{showSingerHomeCheckingErrorDialog}}"/>
    <no-singer-join-error-dialog
            bind:hidetap="handleNoSingerJoinErrorDialog"
            visible="{{showNoSingerJoinErrorDialog}}"/>
    <account-locked-dialog
            visible="{{isLocked}}"/>
</view>
