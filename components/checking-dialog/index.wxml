<view class="checking-dialog" wx:if="{{visible}}" bindtap="handleOutHide">
    <view class="checking-dialog-mask"
          id="mask"/>
    <view class="checking-dialog-body">
        <image src="{{url}}bg.png"
               class="checking-dialog-bg"/>
        <view class="checking-dialog-return"
              bindtap="handleHide"/>
    </view>

</view>
