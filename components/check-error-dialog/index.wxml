<view class="check-error-dialog" wx:if="{{visible}}" bindtap="handleOutHide">
    <view class="check-error-dialog-mask"
          id="mask"/>

    <view class="check-error-dialog-content">
        {{content}}
    </view>
    <view class="check-error-dialog-body">
        <image src="{{url}}bg.png"
               class="check-error-dialog-bg"/>
        <view class="check-error-dialog-return"
              bindtap="handleReWrite"/>
    </view>

</view>
