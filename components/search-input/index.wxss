.input-wrap{
    height: 100%;
    width: 100%;
    background-color: white;

    border-radius:3px;
    color:#004990;
    font-size:19px;
    padding-left:8px;
    font-weight:bolder;

}
.input{
    height: 100%;
    width: 100%;
    padding-left: 0;
}
.search-input > .search-result {
    max-height: 300rpx;
    position: absolute;
    width: 100%;
    top: 100%;
    left: 0;
    right: 0;
    z-index: 22000;
    border: 1px solid yellow;
}



.search-item {
    width: 100%;
    height: 56rpx;
    background-color: white;
    color: #004990;
    font-size: 26rpx;
    display: flex;
    flex-direction: row;
    align-items: center;

}
.search-item:nth-child(odd){
    background-color: lightgray;
}
.ph-cs{
    color:#004990;
    font-size:35rpx;
    margin-left: 0;
    font-weight: bolder;
}
