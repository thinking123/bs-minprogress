.check-error-dialog{
    z-index: 1000;
    width: 100%;
    height: 100%;
    left:0;
    right: 0;
    top:0;
    bottom: 0;
    position: fixed;
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
}

.auth-dialog-ex-mask{
    z-index: 1;
    position: absolute;
    width: 100%;
    height: 100%;
    left:0;
    right: 0;
    top:0;
    bottom: 0;
    background-color: black;
    opacity: 0.8;
}

.auth-dialog-ex-content{
    position: absolute;
    z-index: 3;
    color: white;
    /*top:37.63%;*/
    left:50%;
    top:50%;
    /*height: 25.94%;*/
    transform: translate(-50% , -50%);
    display: flex;
    align-items: center;
    justify-content: center;
    font-weight: bold;
    font-size: 46rpx;
    /*border: 1px solid yellow;*/
}
/*
h 478
*/
.auth-dialog-ex-body{
    position: absolute;
    z-index: 2;
    left:50%;
    top:50%;
    transform: translate(-50% , -50%);
    height: 700rpx;
    width: 576rpx;
}
.auth-dialog-ex-bg{
    width: 100%;
    height: 100%;
}
.auth-dialog-ex-return{
    position: absolute;
    z-index: 2;
    bottom:0;
    width: 422rpx;
    height: 14.43%;
    left:50%;
    transform: translateX(-50%);
    /*border: 1px solid yellow;*/
}


.auth-btn{
    opacity: 0;
    /*border: 0;*/
    /*border: 1px solid yellow;*/
    background-color: transparent;
    color: transparent;
    height: 68rpx;
    width: 298rpx;
    position: absolute;
    z-index: 2;
    left:50%;
    transform: translate(-50% , 0);
    top:554rpx;
}
.auth-btn::before{
    border: 0;
}