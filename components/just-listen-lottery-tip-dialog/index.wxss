.just-listen-lottery-tip-dialog-body{
    position: absolute;
    z-index: 2;
    left:50%;
    top:50%;
    transform: translate(-51.07% , -50%);
    height: 940rpx;
    width: 594rpx;
}
.just-listen-lottery-tip-dialog-bg{
    width: 100%;
    height: 100%;
}
.just-listen-lottery-tip-dialog-return{
    position: absolute;
    z-index: 2;
    bottom:0;
    width: 412rpx;
    height: 14.17%;
    left:50%;
    transform: translateX(-50%);
