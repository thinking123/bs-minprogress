*/
.lottery-dialog-had-prize-body{
    z-index: 2;
    position: absolute;
    transform: translateX(-50%);
    left: 50%;
    width: 676rpx;
    height: 73.01%;
    top:6.75%;
}

.lottery-dialog-had-prize-bg{
    width: 100%;
    height: 100%;
    transform: translateX(-1.07%);
}
.lottery-dialog-had-prize-return{
    position: absolute;
    top:100%;
    left: 50%;
    transform: translate(-50% , 30rpx);
    width: 272rpx;
    height: 92rpx;
}

.lottery-dialog-had-prize-title-wrap{
    z-index: 3;
    position: absolute;
    transform: translateX(-50%);
    left: 50%;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;

    color: white;
    height: 40.63%;
    top:32.68%;
}
.lottery-dialog-had-prize-title{
    display: flex;
    align-items: center;
    justify-content: center;
    letter-spacing: 6rpx;
    font-weight: bold;
    font-size: 54rpx;
    margin-bottom: 20rpx;
}
