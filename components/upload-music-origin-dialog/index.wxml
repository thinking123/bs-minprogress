<view class="upload-music-origin-dialog" wx:if="{{visible}}" bindtap="handleOutHide">
    <view class="upload-music-origin-dialog-mask"
          id="mask"/>
    <view class="upload-music-origin-dialog-body">
        <image src="{{url}}bg.png"
               class="upload-music-origin-dialog-bg"/>
        <view class="upload-music-origin-dialog-btn1"
              data-agree="{{true}}"
              bindtap="handleBtn"/>
        <view class="upload-music-origin-dialog-btn2"
              data-agree="{{false}}"
              bindtap="handleBtn"/>
    </view>

</view>
