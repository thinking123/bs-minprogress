<view class="wrapper" wx:if="{{visible}}" bindtap="handleOutHide">
    <view class="info-confirm-mask" id="mask"></view>
    <!--<view class="body">-->
        <!--<image src="{{url}}bg.png" class="how-to-play-bg"/>-->
        <!--<image src="{{url}}confirm-btn.png" class="confirm-btn" bindtap="handleConfirm"/>-->
        <!--<image src="{{url}}retry-btn.png" class="retry-btn" bindtap="handleRetry"/>-->
        <!---->
    <!--</view>-->
    <image src="{{url}}bg.png" class="info-confirm-bg{{cs == 1 ? '-virtual' : ''}}"/>
    <image src="{{url}}confirm-btn.png" class="confirm-btn  {{cs == 1 ? 'virtual-btn' : ''}}" bindtap="handleConfirm"/>
    <image src="{{url}}retry-btn.png" class="retry-btn {{cs == 1 ? 'virtual-btn' : ''}}" bindtap="handleRetry"/>
    <view class="content-wrap">
        <view class="info-name">
            姓名：{{name}}
        </view>
        <view class="info-phone">
            手机：{{phone}}
        </view>
        <view class="info-qq"
              wx:if="{{cs==2}}">
            QQ：{{qq}}
        </view>
        <view class="info-id-card"
              wx:if="{{cs!=0}}">
            身份证：{{card}}
        </view>
        <view class="info-address"
              wx:if="{{cs!=2}}">
            <view class="address-content-title">
                邮寄地址：
            </view>
            <view class="address-content">
            {{address}}
        </view>
        </view>
    </view>

</view>
